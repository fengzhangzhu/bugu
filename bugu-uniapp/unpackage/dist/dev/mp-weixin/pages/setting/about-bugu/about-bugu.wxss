.body-box {
  min-height: 100vh;
  width: 100vw;
  background: #FFFFFF;
  background-color: transparent;
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
}
.about-logoicon {
  margin: 0 auto;
  margin-top: 20%;
  width: -webkit-fit-content;
  width: fit-content;
  height: -webkit-fit-content;
  height: fit-content;
  text-align: center;
}
.version-number {
  width: -webkit-fit-content;
  width: fit-content;
  text-align: center;
  margin: 20rpx auto;
  font-size: large;
  color: #929292;
}
.hbutliving-content {
  margin-top: 40rpx;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.hbutliving-content .login-logo-image {
  border-radius: 10rpx;
}
.hbutliving-content .hbutliving-text {
  margin-left: 20rpx;
  color: #929292;
  font-size: medium;
}
.update-log {
  margin: 20rpx 0rpx;
}
.about-logo-image-content {
  width: 100%;
  text-align: center;
  margin-top: 5%;
}